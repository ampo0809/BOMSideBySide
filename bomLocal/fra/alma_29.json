]},"note14a":{"id":"note14a","marker":"14a","pid":"117904442","context":"succès","text":"<p data-aid=\"117904493\" id=\"note14a_p1\"><a class=\"scripture-ref\" href=\"/study/scriptures/bofm/alma/17.1-4?lang=fra#p1\">Al 17:1–4</a>.</p>","referenceUris":[{"type":"scripture-ref","href":"/study/scriptures/bofm/alma/17.1-4?lang=fra#p1","text":"Al 17:1–4"}]}}},"pids":[["117904350",{"key":"117904350"}],["117904398",{"key":"117904398"}],["117904401",{"key":"117904401"}],["117904404",{"key":"117904404"}],["117904407",{"key":"117904407"}],["117904410",{"key":"117904410"}],["117904412",{"key":"117904412"}],["117904415",{"key":"117904415"}],["117904418",{"key":"117904418"}],["117904421",{"key":"117904421"}],["117904424",{"key":"117904424"}],["117904427",{"key":"117904427"}],["117904430",{"key":"117904430"}],["117904433",{"key":"117904433"}],["117904436",{"key":"117904436"}],["117904439",{"key":"117904439"}],["117904442",{"key":"117904442"}],["117904445",{"key":"117904445"}],["117904448",{"key":"117904448"}],["117904451",{"key":"117904451"}],["117904454",{"key":"117904454"}],["117904456",{"key":"117904456"}],["117904459",{"key":"117904459"}],["117904463",{"key":"117904463"}],["117904466",{"key":"117904466"}],["117904469",{"key":"117904469"}],["117904472",{"key":"117904472"}],["117904475",{"key":"117904475"}],["117904478",{"key":"117904478"}],["117904481",{"key":"117904481"}],["117904484",{"key":"117904484"}],["117904487",{"key":"117904487"}],["117904490",{"key":"117904490"}],["117904493",{"key":"117904493"}]],"tableOfContentsUri":"/scriptures/bofm","uri":"/scriptures/bofm/alma/29"}