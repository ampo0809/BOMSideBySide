ext":"3 Ne 2:14-16"}]}}},"pids":[["114226891",{"key":"114226891"}],["142121172",{"key":"142121172"}],["114226893",{"key":"114226893"}],["114226894",{"key":"114226894"}],["114226895",{"key":"114226895"}],["114226896",{"key":"114226896"}],["114226897",{"key":"114226897"}],["114226898",{"key":"114226898"}],["114226899",{"key":"114226899"}],["114226900",{"key":"114226900"}],["114226901",{"key":"114226901"}],["114226902",{"key":"114226902"}],["114226903",{"key":"114226903"}],["114226904",{"key":"114226904"}],["114226905",{"key":"114226905"}],["114226906",{"key":"114226906"}],["114226907",{"key":"114226907"}],["114226908",{"key":"114226908"}],["114226909",{"key":"114226909"}],["114226910",{"key":"114226910"}],["114226911",{"key":"114226911"}],["114226912",{"key":"114226912"}],["114226913",{"key":"114226913"}],["114226914",{"key":"114226914"}],["114226915",{"key":"114226915"}],["114226916",{"key":"114226916"}],["114226917",{"key":"114226917"}],["114226918",{"key":"114226918"}],["114226919",{"key":"114226919"}],["114226920",{"key":"114226920"}],["114226921",{"key":"114226921"}],["114226922",{"key":"114226922"}],["114226923",{"key":"114226923"}],["114226924",{"key":"114226924"}]],"tableOfContentsUri":"/scriptures/bofm","uri":"/scriptures/bofm/alma/23"}